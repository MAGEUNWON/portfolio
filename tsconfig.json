{
  "compilerOptions": {
    "target": "ES5",
    "module": "ESNext",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "allowJs": true,
    "moduleResolution": "node",
    "outDir": "./compileJs",
    "baseUrl": "./",
    "paths": {
      "*": ["*"]
    }
  },
  "include": ["./**/*"]
}
